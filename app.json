{
  "expo": {
    "name": "Shymbulak Ski School",
    "slug": "surimobileinfo",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "17",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sagrad.surimobileinfo"
    },
    "android": {
      "package": "com.sagrad.surimobileinfo",
      "googleServicesFile": "./google-services.json",
      "versionCode": 17,
      "permissions": [],
      "useNextNotificationsApi": true
    }
  }
}
